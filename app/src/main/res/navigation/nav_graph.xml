<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.seif.hospitalsystem.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_prototypeFragment"
            app:destination="@id/prototypeFragment" />
    </fragment>
    <fragment
        android:id="@+id/prototypeFragment"
        android:name="com.seif.hospitalsystem.PrototypeFragment"
        android:label="fragment_prototype"
        tools:layout="@layout/fragment_prototype" />
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.seif.hospitalsystem.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" />
</navigation>